<%= fields model: User.new do |form| %>
  <%= form.block :organization_id do %>
    <%= form.label :organization_id, "Select" %>
    <%= form.select :organization_id, Collectivity.pluck(:name, :id), prompt: "" %>
  <% end %>

  <%= form.block :created_at do %>
    <%= form.label :created_at, "Date" %>
    <%= form.date_field :created_at %>
  <% end %>

  <%= form.block :created_at do %>
    <%= form.label :created_at, "Mois" %>
    <%= form.month_field :created_at %>
  <% end %>
<% end %>
